# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Search.index()
GET         /index               controllers.Search.index()
GET         /search              controllers.Search.search()
POST        /login               controllers.Search.login()
GET         /logout              controllers.Search.logout()
POST        /register            controllers.Search.register()

GET         /manage             controllers.Manage.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
